{
  "name": "Custom Mich Pastel Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#3a2d57",
    "editor.foreground": "#E6E6FA",
    "editorLineNumber.foreground": "#B0C4DE",
    "sideBar.background": "#4B3F73",
    "sideBar.foreground": "#E6E6FA",
    "activityBar.background": "#3A2D57",
    "statusBar.background": "#3A2D57",
    "statusBar.foreground": "#FFF5EE"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A2B5FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#F5A8B3"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#8EE5EE"
      }
    },
    {
      "scope": ["constant", "constant.numeric"],
      "settings": {
        "foreground": "#FFD7B0"
      }
    },
    {
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#FA8072",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.language", "variable.language.const", "variable.language.let"],
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#D8BFD8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#FFB6C1"
      }
    },
    {
      "scope": "error",
      "settings": {
        "foreground": "#FF6A6A"
      }
    },
    {
      "scope": "warning",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "class",
      "settings": {
        "foreground": "#BA55D3"
      }
    },
    {
      "scope": "method",
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "scope": "meta.tag",
      "settings": {
        "foreground": "#DDA0DD"
      }
    },
    {
      "scope": ["meta.class", "entity.name.class", "entity.name.tag"],
      "settings": {
        "foreground": "#B19CD9"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#B0E0E6"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#FFB6C1"
      }
    },
    {
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#3CB371"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#FF9D00"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D8BFD8"
      }
    }
  ]
}
