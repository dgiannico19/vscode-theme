{
  "name": "Custom Mich theme",
  "type": "dark",
  "colors": {
    "editor.background": "#F7F2F2",
    "editor.foreground": "#4D4D4D",
    "editorLineNumber.foreground": "#A1C2D4",
    "sideBar.background": "#F0E6E6",
    "sideBar.foreground": "#4B4B4B",
    "activityBar.background": "#E3C9D9",
    "statusBar.background": "#A2DFF7",
    "statusBar.foreground": "#2B2B2B"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#B8D8D2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#F5B5C5"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#B2D7F2"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#B5A5F5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#F4D0A1"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#D1C8F9"
      }
    },
    {
      "scope": "error",
      "settings": {
        "foreground": "#F5A6A6"
      }
    },
    {
      "scope": "warning",
      "settings": {
        "foreground": "#F4E07D"
      }
    }
  ]
}
